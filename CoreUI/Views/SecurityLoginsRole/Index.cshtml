@model IEnumerable<CareerCloud.Pocos.SecurityLoginsRolePoco>

@{
    ViewData["Title"] = "Security Logins Role Index";
}

<h1>Security Logins Role Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SecurityLogin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecurityRole)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SecurityLogin.EmailAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecurityRole.Role)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
