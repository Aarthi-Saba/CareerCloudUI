@model CoreUI.Models.CompanyJobSkill

@{
    ViewData["Title"] = "Company Job Skill Create";
}

<h1>Company Job Skill Create</h1>

<h4>@*CompanyJobSkillPoco*@</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group form-online">
                <table>
                    <tr>
                        <td>
                            <label asp-for="Skill" class="control-label"></label>
                        </td>
                        <td>
                            <input asp-for="Skill" class="form-control" />
